<table>
    <thead>
        <th>Date Submitted</th>
        <th>Employee</th>
        <th>Hours</th>
        <th>Overtime</th>
        <th>Time Entries</th>
        <th>View Entry</th>
    </thead>
    <tbody>
        {{each timeEntries}}
        <tr>
            <td>{{date {{dateSubmitted}} "MM-DD-YYYY"}}</td>
            <td>{{empName}}</td>
            <td>{{hoursCount}}</td>
            <td>{{overtimeCount}}</td>
            <td>{{punchCount}}</td>
            <td>button</td>
        </tr>
        {{/each}}
    </tbody>
</table>